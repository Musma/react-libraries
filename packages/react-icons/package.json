{
  "name": "@musma/react-icons",
  "version": "0.1.1",
  "description": "Musma react icons library",
  "homepage": "https://developers.musma.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/Musma/react-libraries.git",
    "directory": "packages/react-icons"
  },
  "license": "MIT",
  "author": "YoungSu Kim <jklasd2001@musma.net>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/lib.es.js",
      "require": "./dist/lib.umd.js"
    }
  },
  "main": "./dist/lib.umd.js",
  "module": "./dist/lib.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:script": "rimraf ./src/components & node ./src/scripts/build.js",
    "build:optimize": "rimraf ./src/optimized & svgo -q -p 8 -f ./src/raw -o ./src/optimized",
    "build": "yarn build:optimize && yarn build:script",
    "postbuild": "tsc && vite build"
  },
  "devDependencies": {
    "@svgr/cli": "^6.2.1",
    "@svgr/core": "^6.2.1",
    "camelcase": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "svgo": "^2.8.0"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
