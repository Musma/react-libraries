  "devDependencies": {
    "@types/google.maps": "^3.49.2",
    "@types/lodash-es": "^4.17.6",
    "@types/luxon": "^2.3.2",
    "@types/react-copy-to-clipboard": "^5.0.3",
    "autoprefixer": "^10.4.7",
    "classnames": "^2.3.1",
    "lodash-es": "^4.17.21",
    "luxon": "^3.0.1",
    "postcss": "^8.4.14",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^9.0.6",
    "tailwindcss": "^3.1.6"
